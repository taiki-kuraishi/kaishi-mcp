    "superjson": "^2.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.41",
    "@cloudflare/workers-types": "^4.20250619.0",
    "@faker-js/faker": "^9.9.0",
    "@go-task/cli": "^3.44.0",
    "@types/node": "^22.15.30",
    "drizzle-kit": "^0.31.1",
    "knip": "^5.61.2",
    "papaparse": "^5.5.3",
    "supabase": "^2.26.9",
    "typescript": "^5.5.2",
    "vite-tsconfig-paths": "^5.1.4",
